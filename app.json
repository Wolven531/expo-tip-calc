{
  "displayName": "Expo Tip Calc",
  "expo": {
    "name": "Tip Calc",
    "slug": "tip-calc",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "A simple tip calculator developed using Expo",
    "githubUrl": "https://github.com/Wolven531/expo-tip-calc"
  }
}
